body {
    padding: 0;
    margin: 0;
    background: #F7F7F7;
    /* Old browsers */
    background: -moz-linear-gradient(45deg, #F7F7F7 0%, #EAE0D5 100%);
    /* FF3.6-15 */
    background: -webkit-linear-gradient(45deg, #F7F7F7 0%, #EAE0D5 100%);
    /* Chrome10-25,Safari5.1-6 */
    background: linear-gradient(45deg, #F7F7F7 0%, #EAE0D5 100%);
  }
  
  /** Header styles **/
  .header {
    background-image: url(https://lh3.googleusercontent.com/ynsMzZHoaIS6Kv8u1avQDIGvANx-AjOnDf0oTfUh7102srzQ8yV5CqoJ4xazyhrXB73QIr41xb47Ny9vHLhW4P7UKcAvjYjHrzOggQiIY78-gbH61htd4tHBRHkI4D1glQsqC89y-gTKUKyeEtvmLSvDP0UDfMYQ5SlTiTABGBV-CxLEY5QdPwkwei1dRXkJeMJIz0ZoqBBBOWYHb0t7cewaCID6859dLMQZAKLWiSqtdXlH-5onMapTbgPrq2bL25DFMbDUriWq84QaouXIYX5hRC_HN6eLTL6l1KRUe9a3N_LgV_oX7yjrx8R5qwV1CkOgkl0tR2Pxn-mgSbgGCFMQJeeafFYSaZ7mR_xh1j0SWrzo7teSs9mzshEFf1bUjFJeaKqNeopp8UxxNWQk372HorI7Jfp27ff5GnNbBPzduq85_SZt3mDxQeiU1p8EMhQFCA_2bJSIgvyGv6Tu2rXwRl9HQSW2PHvZqdffgJEez1oPtUF8i9ZfxMVIuKuSDxwU3a7a9-8y6iuDLT3R-Z7qJ52u2u5vb2FBz6jFgDsPj8ebdutpFVcf4wPBXq8MV54xMeK3QU5_6Vqw_1-N8aubuSOB9RB0kF3jTg=w636-h848-no
    height: 400px;
    background-position: center center;
  }
  
  .roasting {
    margin: 40px auto;
    padding: 20px 30px 40px 30px;
    background-color: #EAE0D5;
    overflow: auto;
    width: 75%;
    border-radius: 4px;
    align-self: center;
  }
  
  .roasting p {
    color: #938E89;
    text-align: center;
    line-height: 24px;
    font-family: 'Raleway', sans-serif;
    font-size: 16px;
    font-weight: 400;
  }
  
  .spectrum div {
    font: inherit;
    padding: 0 20px;
    color: #FFFFFF;
    overflow: auto;
    opacity: 0.9;
  }
  
  .spectrum p {
    color: #FFFFFF;
    text-align: left;
  }
  
  /* Roast types */
  .green {
    background-color: DarkSeaGreen;
  }
  
  .light {
    background-color: Sienna;
  }
  
  .city {
    background-color: SaddleBrown;
  }
  
  .vienna {
    background-color: Brown;
  }
  
  .italian {
    background-color: Black;
  }
  
  /* Typography */
  h1 {
    color: rgb(250, 248, 248);
    font-family: 'Covered By Your Grace';
    font-size: 100px;
    line-height: 76px;
    margin: 0;
    position: relative;
    text-align: right;
    top: 80%;
  }
  
  h2 {
    color: #E4BB97;
    font-family: 'Raleway', sans-serif;
    font-size: 28px;
    font-weight: 500;
    text-align: left;
    text-transform: uppercase;
  }
  
  h3 {
    color: #41292C;
    font-family: 'Raleway', sans-serif;
    font-size: 22px;
    font-weight: 500;
    text-align: center;
  }
  
  h4 {
    font-family: 'Raleway', sans-serif;
    font-size: 28px;
    font-weight: 700;
    line-height: .2em;
  }
  
  h5 {
    font-family: 'Raleway', sans-serif;
    font-size: 20px;
    line-height: .2em;
    font-weight: 300;
  }
  
  /* Page elements */
  ul {
    margin: 0 auto;
    padding: 0;
    width: 75%;
  }
  
  li {
    border-bottom: 1px solid #E4BB97;
    list-style: none;
    margin: 100px 0px;
    padding-bottom: 60px;
  }
  
  p {
    color: #444444;
    line-height: 32px;
    font-family: 'Raleway', sans-serif;
    font-size: 20px;
    font-weight: 100;
  }
  
  a {
    color: #214E34;
    font-family: 'Raleway', sans-serif;
    font-size: 13px;
    font-weight: 900;
    text-align: left;
    text-transform: uppercase;
    text-decoration: none;
    letter-spacing: 2px;
  }
  footer p {
    text-align: center;
    font-size: small;
}
